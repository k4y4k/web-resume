{
  "name": "kayak-s-resume",
  "version": "1.0.0",
  "private": true,
  "description": "kayak's resume",
  "author": "kayak",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-all": "gatsby build && build-storybook -o public/storybook",
    "format": "yarn prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint": "yarn eslint --ignore-path .gitignore src/",
    "test": "jest --collectCoverage --verbose"
  },
  "dependencies": {
    "@react-icons/all-files": "4.1.0",
    "babel-preset-react-app": "10.0.0",
    "dayjs": "1.10.4",
    "gatsby": "2.31.1",
    "gatsby-plugin-manifest": "2.11.0",
    "gatsby-plugin-offline": "3.9.0",
    "gatsby-plugin-react-helmet": "3.9.0",
    "gatsby-plugin-styled-components": "3.9.0",
    "gatsby-source-filesystem": "2.10.0",
    "gatsby-transformer-json": "2.10.0",
    "gatsby-transformer-remark": "2.15.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-markdown": "5.0.3",
    "styled-components": "5.2.1",
    "tailwindcss": "2.0.2",
    "twin.macro": "2.1.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@storybook/addon-actions": "6.1.16",
    "@storybook/addon-essentials": "6.1.16",
    "@storybook/addon-links": "6.1.16",
    "@storybook/react": "6.1.16",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.4",
    "@types/react": "17.0.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-jsx-remove-data-test-id": "2.1.3",
    "babel-preset-gatsby": "0.11.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{js,ts,jsx,tsx,md,json,css}": "prettier --write"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
