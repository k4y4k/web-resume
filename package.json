{
  "name": "kayak-s-resume",
  "version": "1.0.0",
  "private": true,
  "description": "kayak's resume",
  "author": "kayak",
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build-all": "gatsby build --prefix-paths",
    "format": "yarn prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint": "yarn eslint --ignore-path .gitignore src/",
    "test": "jest --collectCoverage --verbose",
    "fake": "tsc --skipLibCheck --esModuleInterop ./src/helpers/createFakeData.ts && node ./src/helpers/createFakeData.js"
  },
  "dependencies": {
    "@5no/paper-sizes": "1.0.2",
    "@react-icons/all-files": "4.1.0",
    "@types/faker": "5.5.4",
    "babel-preset-react-app": "10.0.0",
    "dayjs": "1.10.4",
    "faker": "5.5.3",
    "gatsby": "3.5.0",
    "gatsby-background-image": "1.5.3",
    "gatsby-plugin-image": "1.5.0",
    "gatsby-plugin-manifest": "3.5.0",
    "gatsby-plugin-offline": "4.5.0",
    "gatsby-plugin-react-helmet": "4.5.0",
    "gatsby-plugin-sharp": "3.5.0",
    "gatsby-plugin-styled-components": "4.5.0",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-filesystem": "3.5.0",
    "gatsby-transformer-json": "3.5.0",
    "gatsby-transformer-sharp": "3.5.0",
    "gbimage-bridge": "0.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-markdown": "6.0.2",
    "short-uuid": "4.1.0",
    "styled-components": "5.3.0",
    "tailwindcss": "2.1.2",
    "twin.macro": "2.4.1",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/react": "17.0.5",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "babel-jest": "26.6.3",
    "babel-plugin-jsx-remove-data-test-id": "3.0.0",
    "babel-preset-gatsby": "1.5.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-styled-components": "7.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{js,ts,jsx,tsx,md,json,css}": "prettier --write"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
